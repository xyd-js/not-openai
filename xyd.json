{
    "theme": {
        "name": "poetry",
        "logo": "/public/assets/logo.svg",
        "favicon": "/public/assets/favicon.svg",
        "maxTocDepth": 2
    },
    "integrations": {
        "search": {
            "orama": true
        }
    },
    "navigation": {
        "header": [
            {
                "name": "API Reference",
                "url": "/api-reference"
            }
        ],
        "sidebar": [
            {
                "route": "api-reference",
                "items": [
                    {
                        "group": "API Reference",
                        "sort": 0,
                        "pages": [
                            "api-reference/introduction"
                        ]
                    }
                ]
            }
        ]
    },
    "api": {
        "openapi": {
            "rest": {
                "source": "./openapi.yaml",
                "route": "api-reference"
            }
        }
    },
    "plugins": [
        "./plugins/pluginOasOpenai"
    ]
}